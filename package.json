{
  "name": "applaid-monorepo",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "web",
    "worker"
  ],
  "scripts": {
    "dev": "npm run dev --workspace web",
    "build": "npm run build --workspace web && npm run build --workspace worker",
    "start": "npm run start --workspace web",
    "worker": "npm run start --workspace worker",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "lint": "npm run lint --workspace web",
    "test": "echo \"No tests yet\""
  },
  "devDependencies": {
    "prisma": "^5.9.0",
    "typescript": "^5.4.0"
  }
}
